{
  "name": "@garron/rchart",
  "version": "1.0.5",
  "description": "react chart",
  "keywords": [
    "chart",
    "G2Plot",
    "g2"
  ],
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "directories": {
    "cjs": "cjs",
    "es": "es"
  },
  "files": [
    "cjs",
    "es",
    "typings",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:es & npm run build:commonjs",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d cjs --extensions .ts,.tsx --delete-dir-on-start",
    "build:docs": "dumi build",
    "build:es": "cross-env BABEL_ENV=es babel src -d es --extensions .ts,.tsx --delete-dir-on-start",
    "build:types": "tsc --declarationDir ./cjs & tsc --declarationDir ./es",
    "dev": "dumi dev",
    "test": "jest -u",
    "test-cov": "jest -u --coverage"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@antv/g2plot": "^2.3.24",
    "@antv/l7plot": "^0.1.0",
    "jest-environment-jsdom": "^29.0.2",
    "react-content-loader": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/string-template": "^1.0.2",
    "@umijs/plugin-analytics": "^0.2.3",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.40",
    "jest": "^29.0.2",
    "jest-canvas-mock": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "string-template": "^1.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2"
  }
}
