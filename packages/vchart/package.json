{
  "name": "@garron/vchart",
  "version": "1.0.0",
  "description": "vue3-chart",
  "keywords": [
    "g2plot",
    "vue3",
    "g2",
    "chart"
  ],
  "license": "MIT",
  "author": "garron",
  "main": "lib/index.js",
  "module": "es/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "files": [
    "lib",
    "es",
    "*.d.ts"
  ],
  "scripts": {
    "build": "npm run build:es && npm run build:commonjs && npm run build:types",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib --extensions .ts,.tsx --delete-dir-on-start",
    "build:docs": "vitepress build docs",
    "build:es": "cross-env BABEL_ENV=commonjs babel src -d es --extensions .ts,.tsx --delete-dir-on-start",
    "build:types": "tsc --declarationDir ./lib && tsc --declarationDir ./es",
    "dev": "vitepress dev docs",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@antv/g2plot": "^2.4.20",
    "element-plus": "^2.2.17",
    "lodash": "^4.17.20",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.2",
    "escape-html": "^1.0.3",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.3",
    "vite": "^3.1.2",
    "vitepress": "^1.0.0-alpha.15",
    "vitepress-demo-editor": "^3.0.2",
    "vitepress-theme-demoblock": "^1.4.2"
  }
}
